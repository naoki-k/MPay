%div
  名前:
  = @user.name
- if @image
  %div
    = image_tag "data:image/jpeg;base64,#{@image}"
%div
  メールアドレス:
  = @user.email
%div
  TEL:
  = @user.tel
%div
  ユーザーコード:
  = @user.code
%div
  Mpayクレカ番号:
  = @credit_payment.decrypted_number
%div
  残高:
  = @credit_payment.balance
%div
  住所:
  = @corporate_information&.address
%div
  詳細:
  = @corporate_information&.detail
